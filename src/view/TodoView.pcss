@import "../pcss/utils.pcss";


.TodoView{
	display: flex;
	flex-direction: row;
	flex: 1;
	position: relative;
	.left-con{
		width: 304px;
		border-right: 1px solid #ddd;
		.left-con-header{
			height: 48px;
			border-bottom: 1px solid #ddd;
			.btn{
				float: right;
				margin: 8px 16px;
			}
		}
	}
	.right-con{
		flex: 1;
		display: flex;
	}
}